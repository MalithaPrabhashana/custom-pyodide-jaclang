{
  "version": 2,
  "builds": [
    {
      "src": "*.html",
      "use": "@vercel/static"
    },
    {
      "src": "*.whl",
      "use": "@vercel/static"
    },
    {
      "src": "pyodide.js",
      "use": "@vercel/static"
    },
    {
      "src": "pyodide.wasm",
      "use": "@vercel/static"
    }
  ],
  "headers": [
    {
      "source": "/pyodide.wasm",
      "headers": [
        {
          "key": "Access-Control-Allow-Origin",
          "value": "*"
        }
      ]
    },
    {
      "source": "/pyodide.js",
      "headers": [
        {
          "key": "Access-Control-Allow-Origin",
          "value": "*"
        }
      ]
    }
  ],
  "routes": [
    {
      "src": "/(.*).html",
      "dest": "/$1.html"
    },
    {
      "src": "/(.*).whl",
      "dest": "/$1.whl"
    },
    {
      "src": "/pyodide.js",
      "dest": "/pyodide.js"
    },
    {
      "src": "/pyodide.wasm",
      "dest": "/pyodide.wasm"
    }
  ]
}
